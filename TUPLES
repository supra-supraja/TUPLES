#Add an item


a=(4,6,2,8,3,1)
b=(4,6,2,8,3,1,9)
print(a)
print(b)
c=a[:5]+(15,20,25)+a[:5]
print(c)


#Length of a tuple


n=tuple(input())
print(n)
a=len(n)
print(a)


#Reverse a Tuple


n=tuple(input())
a=n[::-1]
print(a)


#Repeated element in a tuple


n=int(input())
x=2,4,5,6,2,3,4,4,7
print(x)
temp=x.count(n)
print(temp)
